.workspace-container
  .workspace-vertical-container
    .workspace-vertical-panel
      include toolbar/toolbar
  .workspace-vertical-container.full-relative-size
    .workspace-vertical-panel
      .workspace-container
        .workspace-horizontal-container
          .workspace-horizontal-panel(id="explorer", style="border-bottom: solid 1px #c8c8c8;")
            include explorer/explorer
          .workspace-horizontal-panel.full-relative-size(style="border-bottom: solid 1px #c8c8c8;")
            .workspace-container
              .workspace-vertical-container
                .workspace-vertical-panel
                  include area/area
              .workspace-vertical-container.preview-size-picker-container(style="height: 26px;")
                .workspace-vertical-panel.text-center(style="border-top: solid 1px #c8c8c8;")
                  +CreateReactComponent('FullStackBlend.Components.PreviewSizePicker')
              include footer/footer
          .workspace-horizontal-panel
            include sidebar/sidebar

